#include<iostream>
#include<stdlib.h>
using namespace std;

int result[50];

void find(int num, int** input, int row, int col) {
	if (row < 0 || col < 0)
		return;
	if (row == 0 && col == 0)
		result[num]++;
	// 세로 검색
	for (int i = 1; row - i >= 0; i++) {
		if (row-i >=0 && input[row - i][col] == i)
			find(num, input, row - i, col);
	}
	// 가로검색
	for (int i = 1; col - i >= 0; i++) {
		if (col-i>=0 && input[row][col-i] == i)
			find(num, input, row, col-i);
	}

	return;
}

int main() {
	int i, j, n, size;
	
	cin >> n;
	int *** square = new int**[n];
	
	for (i = 0; i < n; i++) {
		cin >> size;

		// size*size 할당
		square[n] = new int*[size];
		for (i = 0; i < size; i++) {
			square[n][i] = new int[size];
		}

		// 초기화
		for(i=0; i<size; i++)
			for (j = 0; j< size; j++){
				cin >> square[n][i][j];
			} 
	
		// 검색		
		for (i = 0; i < n; i++) {
			find(i, square[n], size - 1, size - 1);
		}
	}

	//결과출력
	for (i = 0; i < n; i++) {
		if ( result[i] >= 1 )
			cout << "YES" << endl;
		else cout << "NO" << endl;
	}
	
	//system("pause");
	return 0;
}
